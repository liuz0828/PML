define function !!aidBOP()
AID CLEAR TEXT ALL

!P0 = POS OF CE WRT WORLD
!P1 = P1BOP OF CE WRT WORLD
!P2 = P2BOP OF CE WRT WORLD
!P0POS = !P0.UP
!P1BOP = !P1.UP
!P2BOP = !P2.UP
AID TEXT NUMBER 100 'EL +$!P0POS' AT $!P0
AID TEXT NUMBER 100 'BOP EL +$!P1BOP' AT $!P1
AID TEXT NUMBER 100 'BOP EL +$!P2BOP' AT $!P2
endfunction